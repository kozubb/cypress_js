{
  "name": "cypress_js",
  "version": "1.0.0",
  "description": "cypressproject",
  "main": "cypress.config.js",
  "scripts": {
    "cy:run": "npx cypress run",
    "cy:open:chrome": "npx cypress open --browser chrome",
    "cy:run:chrome": "npx cypress run --browser chrome --headless",
    "scripts": "cypress run",
    "combine-reports": "mkdir -p cypress/reports/mochareports && mochawesome-merge 'cypress/reports/mocha/*.json' > cypress/reports/mochareports/report.json",
    "generate-report": "mkdir -p cypress/reports/mochareports && marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports --inline",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^15.7.0",
    "cypress-junit-reporter": "^1.3.1",
    "cypress-multi-reporters": "^2.0.5",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.3.2"
  },
  "dependencies": {
    "marge": "^1.0.1"
  }
}
